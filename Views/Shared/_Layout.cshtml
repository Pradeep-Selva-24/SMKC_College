<!DOCTYPE html>
<html lang="zxx" class="theme-light">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!--Bootstrap Css-->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <!--Mean menu.css-->
    <link rel="stylesheet" href="~/css/meanmenu.css">
    <!--Owl carousel-->
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <!--Owl Theme-->
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">
    <!--=== Magnific Popup Min CSS ===-->
    <link rel="stylesheet" href="~/css/magnific-popup.min.css">
    <!--Flat icon-->
    <link rel="stylesheet" href="~/css/flaticon.css">
    <!--Remix icon-->
    <link rel="stylesheet" href="~/css/remixicon.css">
    <!--Odometer-->
    <link rel="stylesheet" href="~/css/odometer.min.css">
    <!--Aos css-->
    <link rel="stylesheet" href="~/css/aos.css">
    <!--Style css-->
    <link rel="stylesheet" href="~/css/style.css">
    <!--Dark css-->
    <link rel="stylesheet" href="~/css/dark.css">
    <!--Responsive css-->
    <link rel="stylesheet" href="~/css/responsive.css">

    <!--Custom css-->
    <link href="~/css/college.css" rel="stylesheet" />

   

    <!--=== Title ===-->
    <title>SMKC - @ViewData["Title"]</title>
    <link rel="icon" type="image/png" href="~/img/logo/favicon.png">
</head>
<body data-aos-easing="ease" data-aos-duration="400" data-aos-delay="0">

    @inject IConfiguration Configuration
    @{
        var ERPURL = Configuration["ERP_Login_URL"];
    }

     <!--=== Jquery Min js ===-->
    @* <script data-cfasync="false" src="../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script> *@
    <script src="~/js/jquery.min.js"></script>
    <!--=== Bootstrap bundle Min js ===-->
    <script src="~/js/bootstrap.bundle.min.js"></script>
    

    <!-- Start Preloader Area -->
    <div class="clsPreloaderArea" style="display:none;">
        <div class="spinner">
            <img src="~/img/loader/murugan.png" />
        </div>
    </div>

    <!-- End Preloader Area -->
    <!--Start Top Header Area-->
    <div class="top-header-area">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-6 col-md-6">
                    <div class="header-left-content">
                        <p>It offers a variety of undergraduate (UG) and postgraduate (PG) courses — about 14–17 courses across streams like Commerce, Science, IT, Business Management and Humanities.</p>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="header-right-content">
                        <div class="list">
                            <ul>
                                @if (!string.IsNullOrEmpty(ERPURL))
                                {
                                    <li><a href="@ERPURL" target="_blank">ERP Login</a></li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--End Top Header Area-->
    <!-- Start Navbar Area -->
    <div class="navbar-area nav-bg-2">
        <div class="mobile-responsive-nav">
            <div class="container">
                <div class="mobile-responsive-menu">
                    <div class="logo">
                        <a href="#">
                            <img src="~/img/logo/logo.png" class="main-logo" lt="logo">
                            <img src="~/img/logo/white-logo.png" class="white-logo" alt="logo">
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="desktop-nav">
            <div class="container-fluid">
                <nav class="navbar navbar-expand-md navbar-light">
                    <a class="navbar-brand" href="~/">
                        <img src="~/img/logo/logo.png" class="main-logo" alt="logo">
                        <img src="~/img/logo/white-logo.png" class="white-logo" alt="logo">
                    </a>
                    <div class="collapse navbar-collapse mean-menu" id="navbarSupportedContent" style="display: block;">
                        <ul class="navbar-nav ms-auto">
                            <li class="nav-item">
                                <a href="~/" class="nav-link clsMenu-Home">Home</a>
                            </li>

                            <li class="nav-item">
                                <a href="#" class="nav-link clsMenu-About dropdown-toggle">About</a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item"><a href="~/VisionMission/VisionMission" class="nav-link clsMenu-VisionMission">Vision & Mission</a></li>
                                    <li class="nav-item"><a href="~/ManagementMessage/ManagementMessage" class="nav-link clsMenu-ManagementMessage">Management Message</a></li>
                                    <li class="nav-item"><a href="~/History/History" class="nav-link clsMenu-History">History & Profile</a></li>
                                    <li class="nav-item"><a href="~/Accreditation/Accreditation" class="nav-link clsMenu-Accreditation">Accreditation / Affiliation</a></li>
                                </ul>
                            </li>

                            <li class="nav-item">
                                <a href="#" class="nav-link clsMenu-Academics dropdown-toggle">Academics</a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item"><a href="~/ProgrammesOffered/ProgrammesOffered" class="nav-link clsMenu-ProgrammesOffered">Programmes Offered</a></li>
                                    <li class="nav-item"><a href="#" class="nav-link clsMenu-AcademicCalendar">Academic Calendar</a></li>
                                    <li class="nav-item"><a href="~/Placement/Placement" class="nav-link clsMenu-Placement">Placement</a></li>
                                </ul>
                            </li>

                            <li class="nav-item">
                                <a href="#" class="nav-link clsMenu-Admissions dropdown-toggle">Admissions</a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item"><a href="~/AdmissionProcedure/AdmissionProcedure" class="nav-link clsMenu-Admission">Admission Procedure</a></li>
                                    <li class="nav-item"><a href="#" class="nav-link clsMenu-Courses">Courses & Eligibility</a></li>
                                    <li class="nav-item"><a href="#" class="nav-link clsMenu-FeeStructure">Fee Structure</a></li>
                                    <li class="nav-item"><a href="#" class="nav-link clsMenu-Prospectus">Apply Online / Prospectus</a></li>
                                </ul>
                            </li>

                            <li class="nav-item">
                                <a href="#" class="nav-link clsMenu-CampusLife dropdown-toggle">Campus Life</a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item"><a href="~/Clubs/Clubs" class="nav-link clsMenu-Clubs">Clubs & Activities</a></li>
                                    <li class="nav-item"><a href="~/EventsGallery/EventsGallery" class="nav-link clsMenu-EventsGallery">Events & Gallery</a></li>
                                    <li class="nav-item"><a href="~/StudentSupport/StudentSupport" class="nav-link clsMenu-StudentSupport">Student Support</a></li>
                                    <li class="nav-item"><a href="~/Alumni/Alumni" class="nav-link clsMenu-Alumni">Alumni</a></li>
                                </ul>
                            </li>

                            <li class="nav-item">
                                <a href="#" class="nav-link clsMenu-QualityAssurance dropdown-toggle">Quality Assurance</a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item"><a href="~/QualityAssurance/IQAC" class="nav-link clsMenu-IQAC">IQAC Committee</a></li>
                                    <li class="nav-item"><a href="~/QualityAssurance/AQAR" class="nav-link clsMenu-AQAR">AQAR Reports</a></li>
                                    <li class="nav-item"><a href="~/QualityAssurance/NAAC" class="nav-link clsMenu-NAAC">NAAC</a></li>
                                    <li class="nav-item"><a href="~/QualityAssurance/NIRF" class="nav-link clsMenu-NIRF">NIRF</a></li>
                                </ul>
                            </li>
                           
                        </ul>

                        <div class="others-options">
                            <div class="icon" style="display: none;">
                                <i class="ri-menu-3-fill" data-bs-toggle="modal" data-bs-target="#sidebarModal"></i>
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>

        <div class="others-option-for-responsive">
            <div class="container">
                <div class="dot-menu">
                    <div class="inner">
                        <div class="icon" style="display: none;">
                            <i class="ri-menu-3-fill" data-bs-toggle="modal" data-bs-target="#sidebarModal"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Navbar Area -->
    <!-- Sidebar Modal -->
    <div class="sidebarModal modal right fade" id="sidebarModal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <button type="button" class="close" data-bs-dismiss="modal"><i class="ri-close-line"></i></button>

                <div class="modal-body">
                    <a href="~/">
                        <img src="~/img/logo/logo.png" class="main-logo" alt="logo">
                        <img src="~/img/logo/white-logo.png" class="white-logo" alt="logo">
                    </a>

                    <div class="sidebar-content">
                        <h3>Get in Touch</h3>
                        <p>
                            We’re here to assist you with admissions, academic queries and general information. Please feel free to contact us or visit our campus.
                        </p>
                    </div>

                    <hr>

                    <div class="sidebar-map">
                        <h3>Our Location</h3>
                        <div class="map-container" style="border-radius:10px; overflow:hidden;">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3895.12345!2d80.2526043!3d13.1330875!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3a52650c9a92bc2d%3A0x4ddf882acacd17ee!2sSree%20Muthukumaraswamy%20College!5e0!3m2!1sen!2sin!4v1700000000000"
                                    width="100%" height="240" style="border:0; border-radius:10px;" allowfullscreen="" loading="lazy">
                            </iframe>


                        </div>
                    </div>

                    <hr>

                    <div class="contact-form">
                        <h3>Feedback / Enquiry Form</h3>

                        <form id="contactForm">
                            <div class="row">
                                <div class="col-lg-12 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="name" class="form-control" required data-error="Please enter your name" placeholder="Your name">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-12 col-md-6">
                                    <div class="form-group">
                                        <input type="email" name="email" class="form-control" required data-error="Please enter your email" placeholder="Your email address">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-12 col-md-12">
                                    <div class="form-group">
                                        <input type="text" name="phone_number" class="form-control" required data-error="Please enter your phone number" placeholder="Your phone number">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-12 col-md-12">
                                    <div class="form-group">
                                        <textarea name="message" class="form-control" cols="30" rows="6" required data-error="Please enter your message" placeholder="Write your message..."></textarea>
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-12 col-md-12">
                                    <button type="submit" class="default-btn">Send Message<span></span></button>
                                    <div id="msgSubmit" class="h3 text-center hidden"></div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </form>
                    </div>

                    <hr>

                    <ul class="sidebar-social-list mt-3">
                        <li><a href="#" target="_blank"><i class="flaticon-facebook"></i></a></li>
                        <li><a href="#" target="_blank"><i class="flaticon-twitter"></i></a></li>
                        <li><a href="#" target="_blank"><i class="flaticon-linkedin"></i></a></li>
                        <li><a href="#" target="_blank"><i class="flaticon-instagram"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- End Sidebar Modal -->

    @RenderBody()

    <!--Start Footer Area-->
    <div class="footer-area pt-100 pb-70">
        <div class="container">
            <div class="row">
                <!-- College Info -->
                <div class="col-lg-6 col-sm-6">
                    <div class="footer-logo-area">
                        <a href="#"><img src="~/img/logo/white-logo.png" alt="College Logo"></a>
                        <p>Sree Muthukumaraswamy College is committed to excellence in education, research and community development. We aim to empower students through knowledge and innovation.</p>
                        <div class="contact-list">
                            <ul>
                                <li>
                                    <a href="tel:+044-25544574">+044-25544574 /</a> <a href="tel:+044-25544577">+044-25544577</a>
                                </li>
                                <li>
                                    <a href="tel:+91 9080208262">+91 9080208262 /</a> <a href="tel:+91 9941286055">+91 9941286055</a>
                                </li>
                                <li>
                                    <a href="mailto:smkc_1997@yahoo.com">smkc_1997@yahoo.com</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

                <!-- Quick Links -->
                <div class="col-lg-3 col-sm-6">
                    <div class="footer-widjet">
                        <h3>Quick Links</h3>
                        <div class="list">
                            <ul>
                                <li><a href="~/Footer/PrivacyPolicy">Privacy Policy</a></li>
                                <li><a href="~/VisionMission/VisionMission">Vision & Mission</a></li>
                                <li><a href="/admissions">Admissions</a></li>
                                <li><a href="~/ProgrammesOffered/ProgrammesOffered">Departments</a></li>
                                <li><a data-bs-toggle="modal" data-bs-target="#sidebarModal" style="cursor: pointer;">Contact Us</a></li>
                            </ul>
                        </div>
                    </div>
                </div>

                <!-- Student Resources -->
                <div class="col-lg-3 col-sm-6">
                    <div class="footer-widjet">
                        <h3>Student Resources</h3>
                        <div class="list">
                            <ul>
                                <li><a href="/academic-calendar">Academic Calendar</a></li>
                                <li><a href="/examinations">Examinations</a></li>
                                <li><a href="/results">Results</a></li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div>
            <div class="shape">
                <img src="~/img/logo/shape-1.png" alt="Image">
            </div>
        </div>
    </div>
    <!--End Footer Area-->
    <!--Start Copyright Area-->
    <div class="copyright-area">
        <div class="container">
            <div class="copyright">
                <div class="row">
                    <div class="col-lg-6 col-md-4">
                        <div class="social-content">
                            <ul>
                                <li><span>Follow Us On</span></li>
                                <li>
                                    <a href="https://www.facebook.com/" target="_blank"><i class="ri-facebook-fill"></i></a>
                                </li>
                                <li>
                                    <a href="https://www.twitter.com/" target="_blank"><i class="ri-twitter-fill"></i></a>
                                </li>
                                <li>
                                    <a href="https://instagram.com/?lang=en" target="_blank"><i class="ri-instagram-line"></i></a>
                                </li>
                                <li>
                                    <a href="https://linkedin.com/?lang=en" target="_blank"><i class="ri-linkedin-fill"></i></a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-8">
                        <div class="copy">
                            <p>© SMKC @DateTime.Now.Year | Sree Muthukumaraswamy College</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--End Copyright Area-->
    <!-- Start Go Top Area -->
    <div class="go-top">
        <i class="ri-arrow-up-s-line"></i>
        <i class="ri-arrow-up-s-line"></i>
    </div>
    <!-- End Go Top Area -->
    
    
    
