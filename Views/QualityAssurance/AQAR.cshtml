@{
    ViewData["Title"] = "AQAR Reports";
}

<div class="aqar-area">
    <!--Start Page Banner-->
    <div class="page-banner-area bg-1">
        <div class="container">
            <div class="page-banner-content">
                <h1>AQAR Reports</h1>
                <ul>
                    <li><a href="~/">Home</a></li>
                    <li>AQAR Reports</li>
                </ul>
            </div>
        </div>
    </div>
    <!--End Page Banner-->
    <!--Start AQAR Reports Area-->
    <div class="ptb-100">
        <div class="container">
            <div class="section-title mb-4">
                <h2>AQAR (Annual Quality Assurance Report)</h2>
                <p>
                    The Annual Quality Assurance Reports (AQAR) document our college’s commitment to continuous improvement and academic excellence.
                    Click below to view or download the reports year-wise.
                </p>
            </div>

            <div class="table-responsive">
                <table class="table table-bordered align-middle text-center">
                    <thead style="background-color: #111d5e; color: #fff;">
                        <tr>
                            <th>Academic Year</th>
                            <th>Report Title</th>
                            <th>Download</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>2023 – 2024</td>
                            <td>AQAR Report 2023-2024</td>
                            <td><a href="~/pdf/aqar/aqar-2023-24.pdf" class="default-btn btn">Download</a></td>
                        </tr>
                        <tr>
                            <td>2022 – 2023</td>
                            <td>AQAR Report 2022-2023</td>
                            <td><a href="~/pdf/aqar/aqar-2022-23.pdf" class="default-btn btn">Download</a></td>
                        </tr>
                        <tr>
                            <td>2021 – 2022</td>
                            <td>AQAR Report 2021-2022</td>
                            <td><a href="~/pdf/aqar/aqar-2021-22.pdf" class="default-btn btn">Download</a></td>
                        </tr>
                        <tr>
                            <td>2020 – 2021</td>
                            <td>AQAR Report 2020-2021</td>
                            <td><a href="~/pdf/aqar/aqar-2020-21.pdf" class="default-btn btn">Download</a></td>
                        </tr>
                        <tr>
                            <td>2019 – 2020</td>
                            <td>AQAR Report 2019-2020</td>
                            <td><a href="~/pdf/aqar/aqar-2019-20.pdf" class="default-btn btn">Download</a></td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <div class="text-center mt-4">
                <p class="text-muted">* All AQAR Reports are submitted to NAC as part of the institution’s quality assurance process.</p>
            </div>
        </div>
    </div>
    <!--End AQAR Reports Area-->
</div>

<script>
    $(document).ready(function(){
        $("#navbarSupportedContent .nav-link").removeClass("active");
        $("#navbarSupportedContent .clsMenu-QualityAssurance").addClass("active");
        $("#navbarSupportedContent .clsMenu-AQAR").addClass("active");
    });
</script>